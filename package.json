{
    "name": "storefy-api",
    "version": "1.0.0",
    "author": "franklin rodriguez",
    "description": "An E-commerce api with nodejs, typescript, typeorm and mysql",
    "main": "index.js",
    "scripts": {
        "start": "export NODE_ENV=production && tsc && tsc-alias && node dist/server.js",
        "dev": "export NODE_ENV=development tsc && concurrently \"tsc-alias -w \" \" nodemon dist/server.js \"",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "prettier": "exec prettier ./src/*** --write"
    },
    "keywords": [],
    "license": "ISC",
    "dependencies": {
        "class-validator": "^0.14.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "mysql": "^2.18.1",
        "typeorm": "^0.3.20",
        "typeorm-naming-strategies": "^4.1.0",
        "typescript": "^5.5.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/helmet": "^4.0.0",
        "@types/morgan": "^1.9.9",
        "@vitest/coverage-v8": "^2.0.5",
        "c8": "^10.1.2",
        "concurrently": "^8.2.2",
        "eslint": "^9.9.1",
        "globals": "^15.9.0",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "supertest": "^7.0.0",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "typescript-eslint": "^8.4.0",
        "vitest": "^2.0.5"
    }
